{
	"name": "ccs-flutter-color",
	"displayName": "Flutter Color",
	"description": "This plugin help you to easily visualize the color.",
	"version": "0.0.7",
	"publisher": "circlecodesolution",
	"keywords": ["futlter","dart","color","andriod","ios"],
	"author": {
		"name": "Nilesh Chavan"
	},
	"icon": "images/logo-color.jpg",
	"galleryBanner": {
		"color": "#356bbc",
		"theme": "dark"
	  },
	"repository": {
		"type": "git",
		"url": "https://github.com/circlecodesolution/vscode-ext-flutter-color-viewer.git"
	  },
	"engines": {
		"vscode": "^1.39.0"
	},
	"categories": [
		"Other"
	],
	"languages": [
		{
			"id": "dart",
			"extensions": [
				".dart"
			],
			"aliases": [
				"dart"
			]
		}
	],
	"activationEvents": [
		"workspaceContains:**/*.dart"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": []
	},
	"homepage": "https://github.com/circlecodesolution/vscode-ext-flutter-color-viewer/blob/master/README.md",
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.39.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"tslint": "^5.12.1",
		"typescript": "^3.3.1",
		"vscode-test": "^1.2.0",
		"webpack": "^4.41.2",
		"webpack-cli": "^3.3.9",
		"serialize-javascript": ">=2.1.1"
	}
}
